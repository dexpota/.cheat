<!-- vim: syntax=Markdown -->
# Cuts text from the cursor until the end of the line
Control + K

# Paste text
Control + Y

# Paste into the terminal
Shift + Insert

# Move to the end of the line
Control + E

# Move to the begin of the line
Control + A

# Move to the next space
Alt + F

# Move to the previous space
Alt + B

# Delete previous word
Alt + Backspace
